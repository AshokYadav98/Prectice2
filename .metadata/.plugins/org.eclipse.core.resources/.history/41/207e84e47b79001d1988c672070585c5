public class multiplication {

	public static void main(String[] args) 
	{
		
		int a=5;
		int b=3;
		
		int sum=0;
		
		 
//		int mul=a*b;
//		
//		System.out.println(mul);

		for(int i=1;i<=3;i++)//1 2 3 4
		{
			sum=sum+a;
			//5=0+5
			//10=5+5
			//15=10+5
			
		}
		System.out.println(sum);

	}

