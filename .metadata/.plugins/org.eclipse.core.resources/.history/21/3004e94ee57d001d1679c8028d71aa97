package logicalPrograms;

public class BlankspaceCount {

	public static void main(String[] args) 
	{
		
		String name="a ur  an  gab  ad  ";
		
		int count=0;
		
		for(int i=name.length()-1;i>=0;i--)
		{
			char t = name.charAt(i);
			
			char space=' ';
			
			if(t==space)
			{
				count++;
			}
		}

	}

}
