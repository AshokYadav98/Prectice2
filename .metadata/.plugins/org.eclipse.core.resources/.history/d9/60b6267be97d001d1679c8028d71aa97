package logicalprogrampractice;

public class Exa1 {

	public static void main(String[] args) 
	{
	//	a b 
	//	a b c 
	//	a b c d 
	//	a b c d e 
	//	a b c d e f 

		
		//total row-->5  a in first row-->1
		
		int a=1;
		int alphabet=97;
		
		for(int i=1;i<=5;i++)
		{
			for(int j=0;j<=a;j++)
			{
				System.out.print((char)(alphabet+j)+" ");
				
			}
			System.out.println();
			a++;
		}
		
		

	}

}
